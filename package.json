{
  "name": "eslint-config-noftalint",
  "version": "1.25.0",
  "description": "Noftaly's eslint configuration",
  "main": "index.js",
  "repository": "https://github.com/noftaly/eslint-config-noftalint",
  "scripts": {
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "find-new-rules": "npx eslint-find-rules --unused ./typescript.js",
    "find-dep-rules": "npx eslint-find-rules --deprecated ./typescript.js",
    "test": "npm run lint && npm run find-new-rules && npm run find-dep-rules",
    "gen": "cd docs && npm run generate-comparison"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "noftalint",
    "eslint-config",
    "eslint-config-noftalint",
    "noftaly"
  ],
  "author": "noftaly",
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=7.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^34.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.29.0",
    "eslint-find-rules": "^3.6.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^34.0.1",
    "typescript": "^4.3.4"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
