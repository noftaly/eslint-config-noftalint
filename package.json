{
  "name": "eslint-config-noftalint",
  "version": "1.16.0",
  "description": "Noftaly's eslint configuration",
  "main": "index.js",
  "repository": "https://github.com/noftaly/eslint-config-noftalint",
  "scripts": {
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "find-new-rules": "npx eslint-find-rules --unused ./index.js",
    "find-dep-rules": "npx eslint-find-rules --deprecated ./index.js",
    "make-docs": "cd docs && npm run make-docs"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "noftalint",
    "eslint-config",
    "eslint-config-noftalint",
    "noftaly"
  ],
  "author": "noftaly",
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^25.0.0"
  },
  "devDependencies": {
    "eslint": "^7.17.0",
    "eslint-find-rules": "^3.6.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^25.0.1"
  },
  "engines": {
    "node": "^10.12.0 || >=12.0.0"
  }
}
