{
  "name": "eslint-config-noftalint",
  "version": "1.19.0",
  "description": "Noftaly's eslint configuration",
  "main": "index.js",
  "repository": "https://github.com/noftaly/eslint-config-noftalint",
  "scripts": {
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "find-new-rules": "npx eslint-find-rules --unused ./typescript.js",
    "find-dep-rules": "npx eslint-find-rules --deprecated ./typescript.js",
    "test": "npm run lint && npm run find-new-rules && npm run find-dep-rules",
    "make-docs": "cd docs && npm run make-docs"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "noftalint",
    "eslint-config",
    "eslint-config-noftalint",
    "noftaly"
  ],
  "author": "noftaly",
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^27.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "eslint-find-rules": "^3.6.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^27.0.0",
    "husky": "^4.3.8",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": "^10.12.0 || >=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test && npm run make-docs"
    }
  }
}
