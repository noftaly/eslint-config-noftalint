{
  "name": "eslint-config-noftalint",
  "version": "2.7.0",
  "description": "Noftaly's eslint configuration",
  "main": "index.js",
  "repository": "https://github.com/noftaly/eslint-config-noftalint",
  "scripts": {
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "find-new-rules": "npx eslint-find-rules --unused ./typescript.js",
    "find-dep-rules": "npx eslint-find-rules --deprecated ./typescript.js",
    "test": "npm run lint && npm run find-new-rules && npm run find-dep-rules",
    "docs": "cd docs && npm run generate-comparison"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "noftalint",
    "eslint-config",
    "eslint-config-noftalint",
    "noftaly"
  ],
  "author": "noftaly",
  "license": "MIT",
  "peerDependencies": {
    "eslint": "^8.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^42.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-find-rules": "^4.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^42.0.0"
  },
  "engines": {
    "node": ">=14.17.0"
  }
}
